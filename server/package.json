    "jsonwebtoken": "^9.0.3",
    "mariadb": "^3.4.5",
    "superjson": "^2.2.2",
    "trpc-to-openapi": "^3.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
